{
  "assim sequential": "true",
  "assim size": "50",
  "assim zero padding": "6",
  "assim pattern": "%eda_member%",
  "assim member template": {
    "resolution": {
      "function space": "StructuredColumns",
      "grid": {
        "type": "regular_lonlat",
        "N": "10"
      },
      "groups": [
        {
          "variables": ["temperature"],
          "levels": "3"
        }
      ],
      "halo": "1"
    },
    "model": {},
    "cost_function": {
      "cost_type": "3D-Var",
      "window_begin": "2010-01-01T12:00:00Z",
      "window_length": "PT0H",
      "variables": ["temperature"],
      "Jb": {
        "Background": {
          "state": [
            {
              "date": "2010-01-01T12:00:00Z",
              "variables": ["temperature"],
              "filepath": "testdata/evil_ens_%eda_member%"
            }
          ]
        },
        "Covariance": {
          "covariance": "SABER",
          "ensemble": [
            {
              "variables": ["temperature"],
              "members": "50",
              "state from template": {
                "template": {
                  "date": "2010-01-01T12:00:00Z",
                  "filepath": "testdata/evil_ens_%member%",
                  "state variables": ["temperature"]
                },
                "pattern": "%member%",
                "zero padding": "6"
              }
            }
          ],
          "saber central block": {
            "saber block name": "Ensemble",
            "inflation value": 1.0,
            "localization": {
              "saber central block": {
                "saber block name": "BUMP_NICAS",
                "read": {
                  "drivers": {
                    "multivariate strategy": "univariate",
                    "read local nicas": "true"
                  },
                  "io": {
                     "overriding nicas file": "testdata/evil_nicas"
                  }
                }
              }
            }
          }
        }
      },
      "Jo": {
        "ObsTypes": [
          {
            "ObsType": "default",
            "ObsData": {
              "ObsDataInScreened": {
                "filepath": "testdata/evil_obs"
              },
              "ObsDataOutScreened": {
                "filepath": "testdata/evil_obs_assim_ensemble_%eda_member%"
              },
              "obsvalue": "ObsValue.%eda_member%"
            },
            "Covariance": {
              "ObsErrorCovariance": {
                "covariance": "diagonal",
                "obserror": "ObsError"
              }
            },
            "variables": ["temperature"]
          }
        ]
      }
    },
    "minimizer": {
      "algorithm": "DRPLanczos"
    },
    "output": {
      "filepath": "testdata/evil_eda_%eda_member%"
    },
    "variational": {
      "iteration": [
        {
          "resolution": {
            "function space": "StructuredColumns",
            "grid": {
              "type": "regular_lonlat",
              "N": "10"
            },
            "groups": [
              {
                "variables": ["temperature"],
                "levels": "3"
              }
            ],
            "halo": "1"
          },
          "timeslot": "PT0H",                
          "linearmodel": {
            "version": "default",
            "tstep": "PT0H",
            "trajectory": {
              "forcing": {
                "variables": ["temperature"]
              }
            }
          },
          "ninner": "20",
          "gradient_norm_reduction": "1e-10",
          "test": "on",
          "diagnostics": {
            "departures": {
              "obsvalue": "ombg"
            }
          }
        }
      ]
    },
    "final": {
      "diagnostics": {
        "departures": {
          "obsvalue": "oman"
        }
      }
    }
  }
}
